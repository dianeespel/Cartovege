# Sous-ensemble des toponymes à afficher
tokeep <- c("Pic du Mascarin", "Baie Américaine", "Baie de la Hébé", "Baie du La Perouse",
"Pointe Basse", "Plateau Jeannel", "Plateau des Petrels", "Vallée des Branloires",
"La Grande Coulée", "Mare aux Eléphants", "Mont Branca",
"Base Alfred Faure", "Baie du Petit Caporal", "Monts Jules Verne",
"Jardin japonais", "Baie du Marin","Mont du Mischief", "Mont des Cratères","Pointe des Moines")
topo <- subset(toponymes, name %in% tokeep)
# Ajouter un champ type de symbole
topo$symbol <- ifelse(topo$name %in% c("Pic du Mascarin", "Mont Branca", "Monts Jules Verne","Mont du Mischief", "Mont des Cratères"),
"mount",  # Triangle noir
ifelse(grepl("^(Plateau|Vallée|La Grande)", topo$name),
"label_only",  # Pas de symbole
"point"))  # Carré noir
# Extraire les coordonnées pour le texte
coords <- st_coordinates(topo)
topo$lon <- coords[, 1]
topo$lat <- coords[, 2]
# Pour la légende des symboles, crée un data frame factice avec les symboles et labels
legend_points <- data.frame(
lon = c(Inf, Inf),   # position hors carte, invisible
lat = c(Inf, Inf),
symbol = factor(c("Mounts", "Toponyms"), levels = c("Mounts", "Toponyms"))
)
# Couleurs personnalisées
colors_custom <- c(
"#5aa382",  # vert (0 m)
"#d9f0d3",  # vert très clair (100 m)
"#fff7bc",  # jaune (~300 m)
"#fdae61",  # orange (~600 m)
"#d7191c",
"#8b0000"# rouge foncé (1000 m)
)
# Correspondance des valeurs normalisées sur 0–1000 m
values_custom <- c(0, 0.1, 0.2, 0.5, 0.7,1)
p_elev <- ggplot() +
geom_raster(data = srtm_df, aes(x = lon, y = lat, fill = elev)) +
scale_fill_gradientn(
colors = colors_custom,
values = values_custom,
name = "Elevation (m a.s.l.)",
limits = c(0, 1000),
breaks = seq(0, 1000, by = 200),
minor_breaks = seq(0, 1000, by = 50),
oob = scales::squish,
na.value = "transparent"
) +
# (le reste de ta carte comme avant, sans changement)
geom_sf(data = contour, fill = NA, color = "black", linewidth = 0.4) +
geom_sf(data = cretes, color = "black", linetype = "dashed", linewidth = 0.7) +
geom_sf(data = subset(topo, symbol == "point"),
shape = 22, fill = "black", color = "black", size = 3) +
geom_sf(data = subset(topo, symbol == "mount"),
shape = 24, fill = "black", color = "black", size = 4) +
geom_text_repel(data = topo,
aes(x = lon, y = lat, label = name),
size = 4,
color = "black",
fontface = "italic",
segment.color = "transparent",
max.overlaps = Inf,
box.padding = 0.4,
point.padding = 0.2,
bg.color = "white",
bg.r = 0.15,
force = 1.5,
min.segment.length = 0,
seed = 42) +
annotation_scale(location = "br", width_hint = 0.3, text_cex = 1) +
annotation_north_arrow(location = "tl", which_north = "true",
style = north_arrow_fancy_orienteering()) +
coord_sf(xlim = c(bbox["xmin"], bbox["xmax"]),
ylim = c(bbox["ymin"], bbox["ymax"]),
expand = FALSE) +
theme_light(base_size = 16) +
theme(
panel.grid.major = element_line(color = "gray80", linetype = "dashed", linewidth = 0.4),
panel.border = element_rect(color = "black", fill = NA, linewidth = 1),
axis.title = element_blank(),
axis.text = element_text(color = "gray20"),
# Position de la légende dans la carte
